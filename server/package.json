{
  "name": "server",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "type": "module",
"scripts": {
  "dev": "nodemon -r dotenv/config src/server.ts",
  "clean": "rimraf dist tsconfig.tsbuildinfo",
  "build": "npm run clean && tsc -p . && npx tsc-alias -p tsconfig.json",
  "postbuild": "node -e \"const fs = require('fs'); try { fs.accessSync('dist/server/src/server.js'); console.log('✅ Build verified: dist/server/src/server.js exists'); } catch(e) { console.error('❌ Build verification failed: dist/server/src/server.js not found'); process.exit(1); }\"",
  "start": "node dist/server/src/server.js",
  "test": "node --test --import tsx tests/intent.test.ts tests/agent.reducer.test.ts tests/places/phase1-services.test.ts tests/city-filter.test.ts tests/street-grouping.test.ts tests/spec-compliance.test.ts",
  "test:resolvers": "node --test --import tsx tests/resolvers.test.ts",
  "test:resolvers:verbose": "node --test --test-reporter=spec --import tsx tests/resolvers.test.ts",
  "test:integration": "node --test --import tsx tests/places-search-integration.test.ts",
  "test:unified": "node --test --import tsx tests/unified-search-integration.test.ts",
  "test:all": "npm test && npm run test:resolvers && npm run test:integration && npm run test:unified",
  "qa": "tsx src/services/search/qa/run-qa.ts",
  "qa:watch": "tsx watch src/services/search/qa/run-qa.ts"
},
  "keywords": [],
  "author": "",
  "license": "ISC",
