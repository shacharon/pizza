    "@aws-sdk/client-dynamodb": "^3.705.0",
    "@aws-sdk/lib-dynamodb": "^3.705.0",
    "@langchain/openai": "^0.6.11",
    "@types/ioredis": "^5.0.0",
    "@types/ws": "^8.18.1",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "helmet": "^8.1.0",
    "ioredis": "^5.9.2",
    "langchain": "^0.3.33",
    "openai": "^5.13.1",
    "pino": "^10.1.0",
    "pino-http": "^11.0.0",
    "pino-pretty": "^13.1.3",
    "rotating-file-stream": "^3.2.7",
    "seedrandom": "^3.0.5",
    "uuid": "^13.0.0",
    "ws": "^8.19.0",
    "rimraf": "^6.0.0",
    "zod": "^4.2.1",
    "zod-to-json-schema": "^3.25.1"
  },
  "devDependencies": {
    "@types/compression": "^1.8.1",
