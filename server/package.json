    "@types/node": "^24.3.0",
    "@types/seedrandom": "^3.0.8",
    "@types/uuid": "^10.0.0",
    "nodemon": "^3.1.10",
    "ts-node": "^10.9.2",
    "tsx": "^4.16.0",
    "typescript": "^5.9.2"
  }
}
