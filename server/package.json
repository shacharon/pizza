{
  "name": "server",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "type": "module",
  "scripts": {
    
  "predev": "node -r dotenv/config -e \"if(!process.env.JWT_SECRET||process.env.JWT_SECRET.length<32){console.error('[P0] JWT_SECRET missing/too short');process.exit(1)}\"",
"dev": "nodemon -r dotenv/config --exec \"node --enable-source-maps --import tsx\" src/server.ts",
    "clean": "rimraf dist tsconfig.tsbuildinfo",
    "build": "npm run clean && tsc -p . && npx tsc-alias -p tsconfig.json",
    "postbuild": "node -e \"const fs = require('fs'); try { fs.accessSync('dist/server/src/server.js'); console.log('✅ Build verified: dist/server/src/server.js exists'); } catch(e) { console.error('❌ Build verification failed: dist/server/src/server.js not found'); process.exit(1); }\"",
    "start": "node dist/server/src/server.js",
    "test": "node --test --import tsx tests/intent.test.ts tests/agent.reducer.test.ts tests/places/phase1-services.test.ts tests/city-filter.test.ts tests/street-grouping.test.ts tests/spec-compliance.test.ts tests/photos.controller.test.ts tests/photos-integration.test.ts",
    "test:security": "node --test --import tsx tests/photos.controller.test.ts tests/photos-integration.test.ts",
    "test:ws": "node --test --import tsx src/infra/websocket/__tests__/*.test.ts",
    "test:ws:unit": "node --test --import tsx src/infra/websocket/__tests__/websocket.config.test.ts src/infra/websocket/__tests__/auth-verifier.test.ts src/infra/websocket/__tests__/subscription-manager.test.ts src/infra/websocket/__tests__/backlog-manager.test.ts src/infra/websocket/__tests__/pending-subscriptions.test.ts src/infra/websocket/__tests__/connection-handler.test.ts",
    "test:ws:e2e": "node --test --import tsx src/infra/websocket/__tests__/websocket-manager.e2e.test.ts",
    "test:resolvers": "node --test --import tsx tests/resolvers.test.ts",
    "test:resolvers:verbose": "node --test --test-reporter=spec --import tsx tests/resolvers.test.ts",
    "test:integration": "node --test --import tsx tests/places-search-integration.test.ts",
    "test:unified": "node --test --import tsx tests/unified-search-integration.test.ts",
    "test:all": "npm test && npm run test:resolvers && npm run test:integration && npm run test:unified",
    "qa": "tsx src/services/search/qa/run-qa.ts",
    "qa:watch": "tsx watch src/services/search/qa/run-qa.ts",
    "verify:prod-contracts": "node scripts/verify-prod-contracts.js",
    "test:lang-propagation": "tsx test-language-propagation.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.705.0",
    "@aws-sdk/lib-dynamodb": "^3.705.0",
    "@langchain/openai": "^0.6.11",
    "@types/ioredis": "^5.0.0",
    "@types/ws": "^8.18.1",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "helmet": "^8.1.0",
    "ioredis": "^5.9.2",
    "jsonwebtoken": "^9.0.3",
    "langchain": "^0.3.33",
    "openai": "^5.13.1",
    "pino": "^10.1.0",
    "pino-http": "^11.0.0",
    "pino-pretty": "^13.1.3",
    "rimraf": "^6.0.0",
    "rotating-file-stream": "^3.2.7",
    "seedrandom": "^3.0.5",
    "uuid": "^13.0.0",
    "ws": "^8.19.0",
    "zod": "^4.2.1",
    "zod-to-json-schema": "^3.25.1"
  },
  "devDependencies": {
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.3.0",
    "@types/seedrandom": "^3.0.8",
    "@types/uuid": "^10.0.0",
    "nodemon": "^3.1.10",
    "ts-node": "^10.9.2",
    "tsx": "^4.16.0",
    "typescript": "^5.9.2"
  }
}
