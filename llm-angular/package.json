{
  "name": "llm-angular",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "jest --watch",
    "test:ci": "jest --ci --coverage",
    "test:preview": "jest --ci && npx jest-preview",
    "prev-secondery": "jest --ci && npx --yes serve reports -l 5520",
    "report-coverage": "jest --ci --coverage",
    "serve:report": "http-server ./reports/jest -p 5173 -c-1",
    "serve-coverage": "npm run report-coverage && npx --yes serve coverage/lcov-report -l 5530",
    "start": "ng serve --proxy-config proxy.conf.json"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^19.2.0",
    "@angular/compiler": "^19.2.0",
    "@angular/core": "^19.2.0",
    "@angular/forms": "^19.2.0",
    "@angular/platform-browser": "^19.2.0",
    "@angular/platform-browser-dynamic": "^19.2.0",
    "@angular/router": "^19.2.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.2.15",
    "@angular/cli": "^19.2.15",
    "@angular/compiler-cli": "^19.2.0",
    "@tailwindcss/postcss": "^4.1.13",
    "@types/jasmine": "~5.1.0",
    "@types/jest": "^29.5.14",
    "autoprefixer": "^10.4.21",
    "jasmine-core": "~5.6.0",
    "jest": "^29.7.0",
    "jest-html-reporters": "^3.1.7",
    "jest-preset-angular": "^14.6.1",
    "jest-preview": "^0.3.1",
    "jsdom": "^26.1.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.17",
    "ts-jest": "^29.4.1",
    "ts-node": "^10.9.2",
    "typescript": "~5.7.2"
  }
}
